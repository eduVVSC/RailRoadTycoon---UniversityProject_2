<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox alignment="CENTER" spacing="10"
      prefWidth="500" prefHeight="400"
      xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1"
      styleClass="box" stylesheets="@../css/styleReadInput.css">


    <Label fx:id="welcomeText" wrapText="true" />

    <ListView fx:id="listOfItems" maxHeight="300.0" maxWidth="400.0" VBox.vgrow="ALWAYS"
              styleClass="rounded-list"/>
    <Button fx:id="buttonExit" onAction="#confirmationBtn" text="Confirm" />
</VBox>

