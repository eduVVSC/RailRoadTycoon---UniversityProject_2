<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="10"
      minWidth="500" prefHeight="400" prefWidth="500"
      styleClass="box" stylesheets="@../css/styleReadInput.css"
      xmlns="http://javafx.com/javafx/17.0.12"
      xmlns:fx="http://javafx.com/fxml/1">

    <Label fx:id="messageLabel" alignment="CENTER" wrapText="true">
    </Label>

    <Button fx:id="btn" styleClass="button" onAction="#confirmationButton" text="Confirm" />
</VBox>
